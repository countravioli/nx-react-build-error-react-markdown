{
  "version": 1,
  "projects": {
    "markdown": {
      "root": "apps/markdown",
      "sourceRoot": "apps/markdown/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/markdown",
            "index": "apps/markdown/src/index.html",
            "main": "apps/markdown/src/main.tsx",
            "polyfills": "apps/markdown/src/polyfills.ts",
            "tsConfig": "apps/markdown/tsconfig.app.json",
            "assets": [
              "apps/markdown/src/favicon.ico",
              "apps/markdown/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/markdown/src/environments/environment.ts",
                  "with": "apps/markdown/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "markdown:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "markdown:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/markdown/.eslintrc",
            "tsConfig": [
              "apps/markdown/tsconfig.app.json",
              "apps/markdown/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/markdown/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/markdown/jest.config.js",
            "tsConfig": "apps/markdown/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "markdown-e2e": {
      "root": "apps/markdown-e2e",
      "sourceRoot": "apps/markdown-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/markdown-e2e/cypress.json",
            "tsConfig": "apps/markdown-e2e/tsconfig.e2e.json",
            "devServerTarget": "markdown:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "markdown:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/markdown-e2e/.eslintrc",
            "tsConfig": ["apps/markdown-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/markdown-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "markdown"
}
